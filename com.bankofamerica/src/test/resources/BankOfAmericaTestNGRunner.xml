<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="BankOfAmerica" verbose="1" >

    <listeners>
        <listener class-name="listeners.AnnotationTransformer"/>
    </listeners>

    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="http://bankofamerica.com"/>


    <test name="Test">


        <classes>
            <class name="TestFiles.TestSearchForCar"></class>
            <class name="TestFiles.TestApplyingForCreditCards"></class>
            <class name="TestFiles.TestApplyingToCdsAccount"></class>
            <class name="TestFiles.TestAutoLoanCalculator"></class>
            <class name="TestFiles.TestConnectWithAdvisor"></class>
            <class name="TestFiles.TestFranchiseFinancing"></class>
            <class name="TestFiles.TestGetMobileApp"></class>
            <class name="TestFiles.TestSearchHome"></class>
            <class name="TestFiles.TestSearchingFinancialCenter"></class>
            <class name="TestFiles.TestSearchJob"></class>

        </classes>
    </test>


</suite>
