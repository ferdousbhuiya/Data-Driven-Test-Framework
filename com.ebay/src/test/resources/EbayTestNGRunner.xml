<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Ebay" verbose="1" >

    <listeners>
        <listener class-name="listeners.AnnotationTransformer"/>
    </listeners>

    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="http://ebay.com"/>
<test name="Test">
    
    <classes>
        <class name="TestFiles.TestAddJeansToCart"></class>
        <class name="TestFiles.TestAddNewVehicle"></class>
        <class name="TestFiles.TestAddToCart"></class>
        <class name="TestFiles.TestAddVehicle"></class>
        <class name="TestFiles.TestChangeVehicle"></class>
        <class name="TestFiles.TestDeleteVehicle"></class>
        <class name="TestFiles.TestRemoveItem"></class>
        <class name="TestFiles.TestSearchItems"></class>
        <class name="TestFiles.TestSearchLevisJeansForMen"></class>
        <class name="TestFiles.TestRemoveJeansFromCart"></class>
    </classes>
</test>
</suite>
